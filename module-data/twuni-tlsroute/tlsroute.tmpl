{{ define "twuniTLSRoute" }}
apiVersion: gateway.networking.k8s.io/v1alpha2
kind: TLSRoute
metadata:
  name: docker-registry
  namespace: korifi
spec:
  hostnames:
  - {{ .TwuniDomain }}
  parentRefs:
  - group: gateway.networking.k8s.io
    kind: Gateway
    name: korifi
    namespace: korifi-gateway
  rules:
  - backendRefs:
    - group: ""
      kind: Service
      name: localregistry-docker-registry
      namespace: cfapi-system
      port: 30050
{{ end }}